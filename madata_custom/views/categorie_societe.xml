<?xml version="1.0" encoding="utf-8"?>
    <odoo>
        <record id='categorie_societe_action' model='ir.actions.act_window'>
            <field name="name">Catégorie Société</field>
            <field name="res_model">categorie.societe</field>
            <field name="view_mode">tree</field>
        </record>
        <!-- <menuitem name="Categorie" id="secteur_activite_cat_menu" parent="contacts.res_partner_menu_config" groups="base.group_user" sequence="20"/> -->
        <!-- <menuitem name="Catégorie Société" id="categorie_societe_menu" parent="secteur_activite_cat_menu" action="categorie_societe_action" groups="base.group_user" sequence="1"/> -->

        <record id="categorie_societe_view_form" model="ir.ui.view">
            <field name="name">Catégorie Société</field>
            <field name="model">categorie.societe</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="categorie_societe_tree" model="ir.ui.view">
            <field name="name">Catégorie Société</field>
            <field name="model">categorie.societe</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name" string="Catégorie Société"/>
                </tree>
            </field>
        </record>
        
